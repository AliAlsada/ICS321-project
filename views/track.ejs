<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style/track.css">
    <title>Track</title>
</head>
<body>
    <section class="step-wizard">
        <ul class="step-wizard-list">
            <% if (history.length > 0) { %> 
                <% history.forEach(record => { %>
                    <li class="step-wizard-item">
                        <span class="progress-count">1</span>
                        <span class="progress-label">Country: <%= record.location_name.country%></span>
                        <span class="progress-label">City: <%= record.location_name.city%></span>
                        <span class="progress-label">Location name: <%= record.location_name.name%></span>
                        <span class="progress-label">Date: <%= record.date %></span>
                        <span class="progress-label">Time: <%= record.time %></span>
                    </li>
            <% }) %> 
            <% } %> 
        </ul>
    </section>

    
</body>
</html>